#include <iostream>
#include <stdlib.h>
using namespace std;

int main ()
{
    int classA = 10;
    int NumberA[classA];
    int classB = 10;
    int NumberB [classB];
    int i;
    double sum = 0;

  
    
    
    for (i = 0; i < classA; i++) {
        sum += NumberA[i];
        NumberA[i] = rand() % 101;
        cout << sum/10<< " ";
        cout<< NumberA[i] << " ";
    }
    for (i = 0; i <classB; ++i){
        sum += NumberB[i];
        NumberB[i]= rand() % 101;
        cout <<sum/10<< " ";
        cout <<NumberB[i]<< " ";
    }
    
    return 0;
}
