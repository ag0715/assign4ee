#include <iostream>
using namespace std;

int main(){
    int n = 2;
    int *myptr;
    
    myptr = new int [n];
    
    for (int i = 0; i < n; ++i){
        cin >> myptr[i];
    }
    
    for (int i=0; i<n; i++){
        cout << myptr[i]<< " ";
    }
    
    
    cout << endl;
    
    delete [ ] myptr;
    
    return 0;
    
}
