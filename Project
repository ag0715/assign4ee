#include <iostream>
#include <fstream>
#include <cstdlib>
#include <string>
#include <math.h>
#include <cfloat>
using namespace std;

double difference(double array[5][6], int r, int c, int r1, int c1){
    return abs(array[r][c]-array[r1][c1]);
}

int main() {
    
    string filename;
    ifstream name;
    double array[5][6];
    int beginRow;
    int row;
    int col;
    cin >> filename;
    filename.append(".txt"); // appends to file name
    name.open(filename);
 
    if(name.is_open()){
    ofstream out("out.txt");
        name >> beginRow;
        row=0;
        col=0;
        
    while(!name.eof()){
        name >> array[row][col];
        col++;
        if(col == 6){
        row++;
        col=0;
    }
        }
        int row = beginRow;
        int col = 1;
        double diff = DBL_MAX;
        int next_row = 0,next_col = 0;
        
        while(true) {
        if(col == 6)
        break;
        cout<<row<<","<<col<<":"<<array[row-1][col-1]<<endl;
        
        if(row > 1){
        diff = difference(array,row-1,col-1,row-2,col);
        next_row = row-1;
        next_col = col+1;
        }
        
        if(row < 5){
        if(difference(array,row-1,col-1,row,col) <= diff){
        diff = difference(array,row-1,col-1,row,col);
        next_row = row+1;
        next_col = col+1;
        }
    }

        if(difference(array,row-1,col-1,row-1,col) <= diff){
        diff = difference(array,row-1,col-1, row-1,col);
        next_row = row;
        next_col = col+1;
        }
            
        row = next_row;
        col = next_col;
        }
        out<<row<<","<<col<<":"<<array[row-1][col-1];
        
        out.close();
        
        name.close();
        
    return 0;
    
}
}
